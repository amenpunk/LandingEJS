<%- include('header'); -%>
<header> <div class="container"> <center> <h1 class="treinta">Lista de Archivos</h1> </center> </div> </header>
<div class="wave"> <div style="height: 150px; overflow: hidden;" ><svg viewBox="0 0 500 150" preserveAspectRatio="none" style="height: 100%; width: 100%;"><path d="M0.00,49.98 C149.99,150.00 349.20,-49.98 500.00,49.98 L500.00,150.00 L0.00,150.00 Z" style="stroke: none; fill: #fff;"></path></svg></div> </div>
<main>
    <center>
        <table class="table">
            <thead> <tr> <th class="tg-0pky"> <center>Nombre</center> </th><th><center>Tipo archivo</center></th> <th class="tg-0pky"><center>Operacion</center></th> </tr> </thead> <tbody>
                <% for(var i=0; i < Files.length; i++) { %>
                <tr>
                    <td> <%= Files[i].file_name %> </td>
                    <td> <%= Files[i].file_type %> </td>
                    <td><a href="/download/<%=Files[i].file_name%>" class="btn btn-success"><center>Descargar</center></button></td>
                </tr>
                <% } %>
            </tbody>
        </table>
    </center>
    <script src="captcha.js"> </script>
</main>
<%- include('footer'); -%>
